{
  "name": "js-package-workflow-example",
  "version": "1.0.0",
  "description": "js package workflow example",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js'",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -c storybook",
    "storybook:build": "build-storybook -c storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --findRelatedTests",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "predeploy": "npm run storybook:build",
    "deploy": "gh-pages -d storybook-static",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test:staged"
    ]
  },
  "keywords": [
    "react",
    "example"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/react": "^5.3.8",
    "@types/jest": "^24.9.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.2",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.2.0"
  },
  "peerDependencies": {
    "react": ">=16.2"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ezezezezez/js-package-workflow-example"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styles.js"
    }
  },
  "homepage": "https://ezezezezez.github.io/js-package-workflow-example/",
  "files": [
    "dist"
  ]
}
